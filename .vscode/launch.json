{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug openautoflutter_example (gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/example/build/linux/x64/release/bundle/openautoflutter_example",
      "args": [],
      "cwd": "${workspaceFolder}/example/build/linux/x64/release/bundle",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": "${workspaceFolder}/example/build/linux/x64/release/bundle/lib"
        }
      ],
      "stopAtEntry": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
          "description": "Enable pretty-printing for gdb"
        }
      ],
      "preLaunchTask": "docker: flutter build linux (example)",
      "externalConsole": false
    }
  ]
}